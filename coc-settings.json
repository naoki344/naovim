{
	"suggest.timeout": 5000,
	"coc.preferences.previewAutoClose": false,
	"suggest.maxPreviewWidth": 100,
	"suggest.snippetIndicator": " >>",
	"suggest.maxCompleteItemCount": 40,
	"suggest.keepCompleteopt": true,
	"suggest.completionItemKindLabels": {
		"function": "\udb80\ude95",
		"method": "\uea8c",
		"variable": "\uea88",
		"constant": "\ueb5d",
		"struct": "\udb81\ude45",
		"class": "\ueb5b",
		"interface": "\ueb61",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"color": "\ue22b",
		"property": "\ueb65",
		"field": "\udb83\udf50",
		"unit": "\uf475",
		"file": "\uf471",
		"value": "\uf8a3",
		"event": "\ufacd",
		"folder": "\uf115",
		"keyword": "\uf893",
		"snippet": "\uf64d",
		"operator": "\uf915",
		"reference": "\uf87a",
		"typeParameter": "\uf278",
		"default": "\uf29c"
	},
	"suggest.virtualText": false,
	"suggest.noselect": true,
	"diagnostic.enable": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextPrefix": " >> ",
	"diagnostic.checkCurrentLine": true,
	"signature.maxWindowHeight": 25,
	"signature.preferShownAbove": false,
	"signature.hideOnTextChange": false,
	"coc.source.file.enable": false,
	"coc.source.buffer.enable": false,
	"languageserver": {
		"bash": {
			"command": "bash-language-server",
			"args": [
				"start"
			],
			"filetypes": [
				"sh"
			],
			"ignoredRootPaths": [
				"~"
			]
		},
		"golang": {
			"command": "gopls",
			"rootPatterns": [
				"go.work",
				"go.mod",
				".vim/",
				".git/",
				".hg/"
			],
			"filetypes": [
				"go"
			],
			"initializationOptions": {
				"usePlaceholders": true
			}
		}
	},
	"python.pythonPath": "~/.venv/fbz/bin/python3",
	"coc.preferences.rootPatterns": [
		".vim",
		".git",
		".hg",
		".projections.json"
	],
	"python.linting.enabled": false,
	"python.linting.pylintEnabled": true,
	"python.linting.mypyEnabled": false,
	"python.linting.mypyArgs": [
		"--ignore-missing-imports",
		"--follow-imports=silent",
		"--show-column-numbers"
	],
	"python.linting.flake8Enabled": true,
	"python.linting.lintOnSave": false,
	"coc.preferences.formatOnSaveFiletypes": [
		"xml",
		".xml",
		"json",
		".json"
	],
	"codeLens.enable": true,
	"jedi": {
		"enable": true,
		"startupMessage": false
	},
	"coc.preferences.jumpCommand": "tabe",
	"python.autoComplete.extraPaths": [
		"${PYTHONPATH}"
	]
}
